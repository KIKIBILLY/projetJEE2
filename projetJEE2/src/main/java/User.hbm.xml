<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="bean.Utilisateur" table="utilisateur">
  <id name="identifiant">
   <column name="identifiant" length="20" />
  </id>
   <property name="credential.email" column="email" length="100" />
  <property name="credential.password" column="password" length="50" />
   <property name="nom" column="nom" length="50" />
  <property name="prenom" column="prenom" length="50"/>
  <property name="numeroTel" column="num_tel" length="50" />
  <property name="adresseHabitation" column="adresse_habitation" length="20" />
   <property name="urlPhoto" column="url_photo" length="25" />
 </class>
</hibernate-mapping>
